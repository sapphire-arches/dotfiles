{
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  },
  "rust-analyzer": {
    "serverPath": "/home/bob_twinkles/.vim/bin/rust-analyzer-from-path.sh",
    "cargo": {
      "runBuildScripts": true
    },
    "procMacro": {
      "enable": true
    }
  }
}
